          <argument>
            <kind>1</kind>
            <string>/// Update movement

// -- Do not touch -- //

stealth = key_down;

// Get axis
var xaxis = (key_right - key_left) * alive;
var yaxis = (-key_space) * alive; // negative because y == 0 at top

// Get direction
dir = point_direction (0, 0, xaxis, yaxis);

// Get length
if (xaxis == 0) {
    xlen = 0;
} else {
    scr_get_face();
    xlen = spd;
}

if (yaxis == 0) {
    ylen = 0;
} else {
    scr_get_face();
    ylen = jump_spd;
    // Control jump rate
        resting = true;
        alarm [0] = room_speed;
}

// Get hspd and vspd
var hspd = lengthdir_x (xlen, dir);
var vspd = lengthdir_y (ylen, dir);

// If not grounded do not allow movement
if (!grounded) { 
    hspd /= 2; // Slows movement in space
    vspd = 0;
}

if (stealth) hspd /= 4; // Slows total speed is in stealth mode

// Apply current movement through impulse
physics_apply_impulse(x, y, (hspd - phy_speed_x), vspd);
</string>
