        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Commence command

if (command != "") {
    // Analyze command
    var protocol = string_char_at(command, 0);
    command = string_delete(command, 1, 1); // remove protocol from command
    var order = scr_substring(command, 0, " ");
    var type = scr_substring(command, 1, " ");
    var amount = scr_substring(command, 2, " ");
    var enabled = scr_substring(command, 3, " ");
    
    var command_report = "CMD :: ";
    
    if (protocol == "/") {
        switch (order) {
            // Spawn an instance
            case "spawn":
            scr_dev_cmd_spawn(type, amount, enabled);
            break;
            case "":
            break;
        }
        command_report += "-" + string(amount) + "- INSTANCE -" + string(order) + "- OF TYPE -" + string(type) + "-";
    } else command_report += "ERROR -unknown command-";
    
    show_debug_message(command_report);
    
    // Clear command
    command = "";
}
</string>
          </argument>
        </arguments>
      </action>
